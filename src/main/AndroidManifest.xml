<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pomato">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pomato">
        <activity android:name=".activities.activities.AddProject"></activity>
        <activity android:name=".activities.activities.Profile" />
        <activity android:name=".activities.activities.EditProfile" />
        <activity android:name=".activities.activities.DashAdmin" />
        <activity android:name=".activities.activities.SignIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.activities.Register" />
        <activity android:name=".activities.activities.DashUser" />
    </application>

</manifest>